[project]
	description = golang OpenGL helper functions
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Tobias Haar (tobscher)
[access "refs/for/refs/*"]
	read = group user/Tobias Haar (tobscher)
	push = group user/Tobias Haar (tobscher)
	label-Code-Review = -2..+2 group user/Tobias Haar (tobscher)
	label-Verified = -1..+1 group user/Tobias Haar (tobscher)
	rebase = group user/Tobias Haar (tobscher)
	removeReviewer = group user/Tobias Haar (tobscher)
	submit = group user/Tobias Haar (tobscher)
[access "refs/heads/*"]
	read = group user/Tobias Haar (tobscher)
	create = group user/Tobias Haar (tobscher)
	push = +force group user/Tobias Haar (tobscher)
	pushMerge = group user/Tobias Haar (tobscher)
[access "refs/tags/*"]
	push = +force group user/Tobias Haar (tobscher)
	pushTag = group user/Tobias Haar (tobscher)
	pushSignedTag = group user/Tobias Haar (tobscher)
